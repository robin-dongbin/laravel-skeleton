{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "cloudtay/laravel-ripple": "dev-main",
    "dedoc/scramble": "^v0.12.5",
    "filament/filament": "^3.2",
    "inertiajs/inertia-laravel": "^2.0",
    "lacodix/laravel-model-filter": "^3.2",
    "laravel/framework": "^11.9",
    "laravel/horizon": "^5.29",
    "laravel/octane": "^2.7",
    "laravel/pulse": "^1.3.1",
    "laravel/sanctum": "^4.0",
    "laravel/telescope": "^5.2",
    "laravel/tinker": "^2.10.1",
    "moonlydays/inertia-routed-modals": "^1.1",
    "opcodesio/log-viewer": "^3.11",
    "sentry/sentry-laravel": "^4.9",
    "spatie/laravel-navigation": "^1.3",
    "tightenco/ziggy": "^2.5",
    "workerman/workerman": "^5.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.14",
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^3.0.2",
    "laravel-lang/common": "^6.3",
    "laravel/pail": "^1.1",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.1",
    "peckphp/peck": "^0.1.1",
    "pestphp/pest": "^3.7.4",
    "pestphp/pest-plugin-laravel": "^3.0",
    "pestphp/pest-plugin-stressless": "^3.1",
    "pestphp/pest-plugin-type-coverage": "^3.2.3",
    "phpstan/phpstan": "2.1.2",
    "phpunit/phpunit": "^11.0.1",
    "rector/rector": "^2.0.7",
    "spatie/laravel-ray": "^1.37"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
    ],
    "lint": "pint",
    "refactor": "rector",
    "test:type-coverage": "pest --type-coverage --min=100",
    "test:typos": "peck",
    "test:lint": "pint --test",
    "test:unit": "pest --parallel --exactly=100",
    "test:types": "phpstan",
    "test:refactor": "rector --dry-run",
    "test": [
      "@test:refactor",
      "@test:unit",
      "@test:lint"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
